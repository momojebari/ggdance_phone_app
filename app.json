{
  "expo": {
    "name": "GG Dance Parent",
    "slug": "gg-dance-parent",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "dark",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#0f172a"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.ggdance.parent"
    },
    "android": {
      "package": "com.ggdance.parent"
    },
    "plugins": [
      [
        "expo-notifications",
        {
          "color": "#eab308"
        }
      ],
      "expo-asset"
    ],
    "extra": {
      "eas": {
        "projectId": "60ba79b5-6b27-44cd-bb7d-5dcb43874b1a"
      }
    }
  }
}
